{
  "testEnvironment": "jsdom",
  "transform": {
    "^.+\\.(js|jsx)$": "babel-jest"
  },
  "moduleFileExtensions": ["js", "jsx", "json", "node"],
  "setupFilesAfterEnv": ["./setupTests.js"],
  "moduleNameMapper": {
    "^.+\\.(css|less|scss)$": "identity-obj-proxy",
    "\\.svg": "<rootDir>/src/__test__/__mocks__/svg.js"
  }
}
